<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="android.lifeistech.com.braillewords.toJActivity"
    android:orientation="vertical"
    android:weightSum="20"
    android:background="@drawable/background">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="翻訳結果"
        android:id="@+id/textView4"
        android:layout_weight="1" />

    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/toJResult"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:hint="翻訳結果が表示されます"
            android:textAppearance="@android:style/TextAppearance.Large"
        android:layout_weight="1"
        android:onClick="copy" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="一文字消去"
            android:id="@+id/toJback1"
            android:onClick="del1" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="全消去"
            android:id="@+id/toJbackAll"
            android:onClick="delAll" />

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="12"></LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="5"
        android:layout_weight="5">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:weightSum="3"
            android:id="@+id/linearLayout1">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="#00000000">

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button1"
                    android:layout_weight="1"
                    android:scaleType="center"
                    android:onClick="button1"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />

                <ImageButton
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button2"
                    android:layout_weight="1"
                    android:onClick="button2"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="#00000000">

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button3"
                    android:layout_weight="1"
                    android:onClick="button3"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button4"
                    android:layout_weight="1"
                    android:onClick="button4"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="#00000000">

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button5"
                    android:layout_weight="1"
                    android:onClick="button5"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button6"
                    android:layout_weight="1"
                    android:onClick="button6"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"></LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:weightSum="3"
            android:id="@+id/linearLayout2">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="#00000000">

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button7"
                    android:layout_weight="1"
                    android:onClick="button7"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button8"
                    android:layout_weight="1"
                    android:onClick="button8"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="#00000000">

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button9"
                    android:layout_weight="1"
                    android:onClick="button9"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button10"
                    android:layout_weight="1"
                    android:onClick="button10"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:weightSum="2"
                android:background="#00000000">

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button11"
                    android:layout_weight="1"
                    android:onClick="button11"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />

                <ImageButton
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="New Button"
                    android:id="@+id/button12"
                    android:layout_weight="1"
                    android:onClick="button12"
                    android:src="@drawable/buttonchange"
                    android:background="#00000000" />
            </LinearLayout>
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
